<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="144.0" prefWidth="350.0" stylesheets="@../../css/dialog.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="husseinm.opdss.DialogController">
   <children>
      <Label styleClass="title" text="New Project">
         <font>
            <Font size="16.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="textField" promptText="Name of your project">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </VBox.margin>
      </TextField>
      <Label fx:id="lbl_error" textFill="RED">
         <VBox.margin>
            <Insets left="10.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER" spacing="20.0">
         <children>
            <JFXButton fx:id="button_cancel" defaultButton="true" prefHeight="30.0" prefWidth="120.0" text="Cancel" />
            <JFXButton fx:id="button_ok" defaultButton="true" prefHeight="30.0" prefWidth="120.0" text="Ok" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
